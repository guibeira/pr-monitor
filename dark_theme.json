{
  "name": "dark_theme",
  "author": "Goose",
  "version": "1.0.0",
  "comment": "A dark theme with a dark background and light text.",
  "colors": {
    "background": "#1E1E1E",
    "foreground": "#D4D4D4",
    "selectionBackground": "#007AFF",
    "selectionForeground": "#FFFFFF",
    "cursorColor": "#D4D4D4",
